{
  "name": "homelab-manager",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "typecheck": "tsc --noEmit",
    "build": "bun run typecheck && vite build",
    "preview": "vite preview",
    "test": "cross-env-shell \"bun test --coverage 2>&1 | node scripts/check-coverage.js\"",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:coverage:check": "node scripts/check-coverage.js",
    "worker": "bun src/worker/collector.ts",
    "cleanup": "bun src/worker/cleanup.ts",
    "dev:docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml down",
    "dev:docker:restart": "docker compose -f docker-compose.dev.yml down && docker compose -f docker-compose.dev.yml up -d",
    "icons:download": "bun scripts/download-icons.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.2.8",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/material": "^7.3.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.11",
    "@tanstack/react-router": "^1.159.6",
    "@tanstack/react-router-devtools": "^1.159.6",
    "@tanstack/react-router-ssr-query": "^1.159.6",
    "@tanstack/react-start": "^1.159.6",
    "@tanstack/router-plugin": "^1.159.6",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/dockerode": "^4.0.1",
    "@types/ssh2": "^1.15.5",
    "@types/ssh2-streams": "^0.1.13",
    "dockerode": "^4.0.9",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.6",
    "lucide-react": "^0.561.0",
    "nitro": "npm:nitro-nightly@latest",
    "pg": "^8.18.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "ssh2": "^1.17.0",
    "tailwindcss": "^4.1.18",
    "vite-tsconfig-paths": "^6.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^20.6.1",
    "@tanstack/devtools-vite": "^0.3.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/bun": "^1.3.9",
    "@types/node": "^22.19.11",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "cross-env": "^10.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}